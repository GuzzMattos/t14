{
  "expo": {
    "name": "EqualPay",
    "slug": "equalpay",
    "scheme": "equalpay",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.equalpay.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.equalpay.app",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/notification.wav"
          ]
        }
      ]
    ],
    "web": {
      "bundler": "metro"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "ddd830ef-a1d1-43cd-b955-9b3299a171e8"
      }
    },
    "owner": "dameuhash"
  }
}
